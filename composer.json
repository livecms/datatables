{
    "name": "mrofi/livecms-datatables",
    "description": "Live CMS Datatables",
    "homepage": "https://github.com/mrofi/livecms-datatables",
    "license": "MIT",
    "authors": [
        {
            "name": "Mokhamad Rofiudin",
            "email": "mokh.rofiudin@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/contracts": "5.4.*|5.5.*|5.6.*",
        "illuminate/support": "5.4.*|5.5.*|5.6.*",
        "yajra/laravel-datatables-oracle": "~8.0"
    },
    "autoload": {
        "psr-4": {
            "LiveCMS\\DataTables\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LiveCMS\\DataTables\\DataTablesServiceProvider"
            ],
            "aliases": {
                "DataTables": "LiveCMS\\DataTables\\Facades\\DataTables"
            }
        }
    }
}
